    "@babel/runtime": "^7.1.5",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1"
  },
  "bugs": {
    "url": "https://github.com/josefzamrzla/sdic/issues"
